{% extends "navbar.html.j2" %}


{% set Classstyleclass = "list-group-item list-group-item-action " %}
{% set itermstyleclass = "list-group-item list-group-item-action list-group-item-info " %}



{% block head %}
<style>
.fixed-box {
  min-height: 300px;
  max-height: 300px;
  min-width:  300px;
  overflow-y: scroll;
  border:groove;
}

.fixed-actionbox{
  min-height: 600px;
  max-height: 600px;
}
</style>
{% endblock %}






{% block main %}
<div class="jumbotron">
  <div class="container">
    <div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups">
      {% block actionbar %}
      {% endblock %}
    </div>
  </div>
</div>



{% block Class_Item_List_frame %}
<div class='container '>
    <div class='row'>

        <div class="list-group fixed-box col6">
            <center style="background-color: grey;">
              <strong >Class</strong>
            </center>
            {% block classlist %}
            {% endblock %}
        </div>

        <div class="list-group fixed-box col6">
            <center style="background-color: grey;">
              <strong>Item</strong>
            </center>
            {% block itemlist %}
            {% endblock %}
        </div>

    </div>
</div>
{% endblock %}







<div class='container'>
    {% block edititerm %}
    {% endblock %}
</div>






{% block scripts %}
{% endblock %}

{% endblock %}
